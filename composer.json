{
    "name": "movistarplus/acd",
    "description": "Editor de contenidos",
    "keywords": [
        "movistarplus",
        "content manager",
        "acd"
    ],
    "license": "Mozilla Public License 2.0",
    "type": "project",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Acd\\": "app"
        },
        "files": [
            "config.php"
        ]
    },
},
"autoload-dev": {
    "psr-4": {
        "Movistarplus\\Acd\\Tests\\": "tests"
    }
},
"scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
},
"config": {
    "sort-packages": true
}
}